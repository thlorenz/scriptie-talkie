{
  "name": "scriptie-talkie",
  "version": "0.4.4",
  "description": "Makes your code tell you what the intermediate results are when executing a script.",
  "main": "scriptie-talkie.js",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "cardinal": "~0.3.1",
    "esprima": "~1.0.2",
    "difflet": "thlorenz/difflet",
    "clone": "~0.1.5",
    "ansicolors": "~0.2.1",
    "require-like": "~0.1.2",
    "deep-is": "~0.1.0",
    "snippetify": "~0.2.0"
  },
  "devDependencies": {
    "through": "~2.2.7",
    "tape": "~0.3.3",
    "tap": "~0.4.1",
    "brfs": "0.0.4",
    "hypernal": "~0.1.2",
    "browserify": "~2.12.0",
    "ecstatic": "~0.4.2",
    "mold-source-map": "~0.2.0"
  },
  "scripts": {
    "test": "tap test/*.js",
    "tape": "node test/browser/build && open test/browser/index.html"
  },
  "bin": {
    "scriptie-talkie": "./bin/scriptie-talkie.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/scriptie-talkie.git"
  },
  "keywords": [
    "debug",
    "script",
    "inspect"
  ],
  "author": "Thorsten Lorenz <thlorenz@gmx.de> (thlorenz.com)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "gitHead": "315c3a35223a0bd1eaa95399dc539ee681def21c",
  "testling": {
    "preprocess": "node test/browser/build.js",
    "html": "test/browser/index.html",
    "browsers": [
      "ie/6..latest",
      "firefox/3..5",
      "firefox/19..nightly",
      "chrome/4..7",
      "chrome/24..canary",
      "opera/10..next",
      "safari/4..latest",
      "iphone/6",
      "ipad/6"
    ]
  }
}
